{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla and NodeJS",
  "author": "SDC",
  "dependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "aws-sdk": "^2.804.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.1.1",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "pug": "^3.0.0",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static'",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src\\assets','src\\static','src\\webpack.config.js'",
    "copyAll": "xcopy /i/s/y src\\static build\\static && xcopy /i/s/y src\\views build\\views",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rmdir /s/q build",
    "turnel": "ngrok http 4000",
    "start": "node build/init.js"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "mini-css-extract-plugin": "^1.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "sass-loader": "^10.0.4"
  }
}
